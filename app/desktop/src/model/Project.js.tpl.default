/**
 * This entity represents a project which is a container for {appName}s.
 */
Ext.define('{appName}.model.Project', {
    extend: '{appName}.model.Base',

    fields: [
        'name',
        { name: 'organizationId', reference: 'Organization' }, {
            name: 'leadId',
            unique: true,
            reference: 'User'
        }
    ]
});
